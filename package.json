{
  "name": "quickstart-node-npm-environment",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "run-p type-check build-only",
    "build-only": "vite build --base=/node-npm-environment/ --outDir target/dist",  
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "cache-clean-force": "npm cache clean --force"
  },
  "dependencies": {
    "pinia": "~3.0.2",
    "vue": "~3.5.13",
    "vue-router": "~4.5.1",
    "axios": "~1.9.0",
    "core-js": "~3.42.0",
    "@popperjs/core": "~2.11.8",
    "bootstrap": "~5.3.6",
    "@vuelidate/core": "~2.0.3",
    "@vuelidate/validators": "~2.0.4",
    "vue-i18n": "~11.1.3",
    "dayjs": "~1.11.13",
    "deepmerge": "~4.3.1"
  },
  "devDependencies": {
    "@babel/types": "~7.27.1",
    "@rushstack/eslint-patch": "~1.11.0",
    "@tsconfig/node20": "~20.1.5",
    "@types/node": "~22.15.17",
    "@vue/runtime-dom": "~3.5.13",
    "@vue/tsconfig": "~0.7.0",
    "eslint": "~9.26.0",
    "npm-run-all": "4.1.5",
    "prettier": "~3.5.3",
    "sass": "~1.87.0",
    "sass-loader": "~13.3.2",
    "vue-loader": "~17.4.2",
    "typescript": "~5.8.3",
    "vite": "~6.3.5",
    "vue-tsc": "~2.2.10"
  }
}
